    <div id="content">
      <div class="header">
        <h1 class="doc-title">Welcome</h1>
        <h1 class="doc-title1">Doctor</h1>
        <hr class="hr1">
        <hr class="hr2">
      </div>
      <div class="three">
        <div class="container">
          <div class="card">
            <div class="card-text">

              <form name="f1" action="/autodoc/docdash" method="POST">
                <center>
                  <h1> PRESCRIPTION FORM </h1>
                  <table class="doctorform">
                    <tr>
                      <td>Name</td>
                      <td><input type="text" name="name" id="uname" ></td>
                      <td>
                        <p id="xyz"></p>
                      </td>
                    </tr>
                    <tr>
                      <td>Sex</td>
                      <td><input TYPE="RADIO"  VALUE="Male">MALE</td>
                      <td><input TYPE="RADIO"  VALUE="FEMALE">FEMALE</td>
                    </tr>
                    <tr>
                      <td>Email-ID</td>
                      <td><input type="text" name="email" id="mailid" ></td>
                      <td>
                        <p id="def"></p>
                      </td>
                    </tr>
                    <tr>
                      <td>Age</td>
                      <td>
                        <input type="text" id="age"  ></td>
                      <td>
                        <p id="efg"></p>
                      </td>
                    </tr>
                    <tr>
                      <td>Medicines</td>
                      <td><textarea id="add" name="med" rows="4" cols="50"></textarea></td>
                      <td>
                        <p id="efg"></p>
                      </td>
                    </tr>
                    <tr>
                      <td></td>
                    <tr>
                      <td></td>
                      <td><input type="Submit" id="submit" ></td>
                      <td>
                        <p id="ijk"></p>
                      </td>
                    </tr>
                  </table>

                </center>
              </form>

            </div>
          </div>
        </div>
